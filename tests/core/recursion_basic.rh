// infinite loop if i_start > end
void basic_recursion_test(int i, int end) {
    put(i);
    if (i != end) {
        i += 1;
        basic_recursion_test(i, end);
    }
}

int recursion_return(int i, int end) {
    if (i != end) {
        i += 1;
        int y = recursion_return(i, end);
        return(y);
    }
    return(i);
}

//basic_recursion_test('0', '4');
put('i');

int y = recursion_return('0', '3');
